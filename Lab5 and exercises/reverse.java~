class reverse{
  public static main void(String[] args){
    Scanner sc = new Scanner(System.in);
    
    int size = sc.nextInt();
    int[] arr = new int[size];
    
    for(int i=0; i<size; i++){
      arr[i] = sc.nextInt();
    }
    
    arr = reverseArr(arr, 0, size-1);
    
    for(int j=0; j<size; j++){
      System.out.print(arr[j] + " ");
    }
  }
  
  public int[] reverseArr(int[] arr, int i, int size-1){
    int temp;
    if(i==k || k<i)
      return arr;
    else{
      temp = arr[i];
      arr[i] = arr[k];
      arr[k] = temp;
      return reverseArr(arr, i+1, k-1);
    }
  }
}
      